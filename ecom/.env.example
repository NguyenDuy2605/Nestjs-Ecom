# Database connection
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE?schema=public"

# App info
APP_NAME='Ecom'

# Payment
PAYMENT_API_KEY=your_payment_api_key_here

# JWT
ACCESS_TOKEN_EXPIRES_IN=1h
REFRESH_TOKEN_EXPIRES_IN=1d
ACCESS_TOKEN_SECRET=your_access_token_secret_here
REFRESH_TOKEN_SECRET=your_refresh_token_secret_here

# Admin credentials
ADMIN_NAME=your_admin_name_here
ADMIN_PASSWORD=your_admin_password_here
ADMIN_EMAIL=admin_email@example.com
ADMIN_PHONE_NUMBER=0123456789

# OTP
OTP_EXPIRES_IN=5m
RESEND_API_KEY=your_resend_api_key_here

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback
GOOGLE_CLIENT_REDIRECT_URI=http://localhost:3300/oauth-google-callback

# Static files
PREFIX_STATIC_ENPOINT=http://localhost:3000/media/static

# S3 / Cloud storage
S3_BUCKET_NAME=your_bucket_name_here
S3_REGION=your_region_here
S3_ACCESS_KEY=your_access_key_here
S3_SECRET_KEY=your_secret_key_here
S3_ENPOINT=https://your_s3_endpoint_here

# Redis
REDIS_URL=redis://localhost:6379
